{{#contacts}}
    <div class="containerContact__memberContainer">
        <div class="memberContainer__signature">
            <div class="memberContainer__signature--drawn">
                <svg viewBox="{{member.viewbox_signature}}" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                        <g class="signature--kevin" stroke="#000000" stroke-width="2">
                            <path d="{{member.signature}}" id="Path"></path>
                        </g>
                    </g>
                </svg>
            </div>
            <div class="memberContainer__signature--name">{{member.name}}</div>
            <div class="memberContainer__signature--role">{{member.role}}</div>
            <div class="memberContainer__signature--rs">
                <!-- If member have "twitter/dribble/etc..." show the logo if not show nothing -->
                {{#if member.twitter}}
                <a href="https://twitter.com/{{member.twitter_link}}"><svg class="memberContainer__signature--rsTwitter" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 612 612"><path d="{{member.twitter}}" /></svg></a>
                {{/if}}
                {{#if member.dribble}}
                <a href="https://dribble.com/{{member.dribble_link}}"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 438.533 438.533"><path d="{{member.dribble}}"/></a>
                {{/if}}
                {{#if member.behance}}
                <a href="https://www.behance.net/{{member.behance_link}}"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 49.652 49.652"><path d="{{member.behance1}}"></path><path d="{{member.behance2}}"></path><path d="{{member.behance3}}"></path><path d="{{member.behance}}"></path></svg></a>
                {{/if}}
                {{#if member.codepen}}
                  <a href="http://codepen.io/{{member.codepen_link}}"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 31.665 31.665"><path d="{{member.codepen}}"/></svg></a>
                {{/if}}
                {{#if member.github}}
                <a href="https://github.com/{{member.github_link}}"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path d="{{member.github}}"/></svg></a>
                {{/if}}

            </div>
        </div>
        <div class="memberContainer__image">
            <img src="dist/images/{{member.photo}}" alt="">
        </div>
    </div>
{{/contacts}}
